<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
			prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<ToolBar prefWidth="200.0" BorderPane.alignment="CENTER">
			<items>
				<Label text="Oops!">
					<font>
						<Font name="System Bold" size="24.0"/>
					</font>
				</Label>
				<Label fx:id="titleLabel" text="">
					<font>
						<Font size="24.0"/>
					</font>
				</Label>
			</items>
		</ToolBar>
	</top>
	<center>
		<TextArea fx:id="messageArea" editable="false" prefHeight="200.0" prefWidth="200.0" promptText="Error message"
				  BorderPane.alignment="CENTER">
			<font>
				<Font size="18.0"/>
			</font>
		</TextArea>
	</center>
	<bottom>
		<HBox alignment="CENTER" BorderPane.alignment="CENTER">
			<children>
				<Button mnemonicParsing="false" onAction="#close" prefHeight="26.0" prefWidth="100.0" text="OK"/>
			</children>
			<BorderPane.margin>
				<Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
			</BorderPane.margin>
		</HBox>
	</bottom>
</BorderPane>
